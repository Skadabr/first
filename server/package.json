{
  "scripts": {
    "typecheck": "tsc --lib es2017 --target es5 --noEmit --pretty --experimentalDecorators",
    "prestart": "tsc --lib es2017 --target es5 --sourceMap  --experimentalDecorators --outDir dist src/*.ts src/**/*.ts src/**/**/*.ts",
    "start": "node dist",
    "test:ui": "mocha --require babel-register -b -t 3000 -R min --recursive test/acceptance",
    "test:ui:i": "mocha --require babel-register -b -t 3000 test/acceptance/init.test.js",
    "prestart:dev": "tsc --pretty --lib es2017 --target es5 --sourceMap  --experimentalDecorators --outDir dist -w  src/*.ts src/*.ts src/**/*.ts src/**/**/*.ts > typecheck.log &",
    "start:dev": "tsc --pretty --lib es2017 --target es5 --sourceMap --pretty  --experimentalDecorators --outDir dist  src/*.ts src/*.ts src/**/*.ts src/**/**/*.ts > typecheck.log && nodemon -r source-map-support/register dist",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "deep-copy": "^1.4.2",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "lodash.flatmap": "^4.5.0",
    "moment": "^2.20.1",
    "mongoose": "^4.13.9",
    "mongoose-unique-validator": "^1.0.6",
    "morgan": "^1.9.0",
    "only": "^0.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "pino": "^4.10.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/node": "^8.9.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bind-decorator": "^1.0.11",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.4.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.9",
    "prettier": "^1.9.2",
    "puppeteer": "^1.0.0",
    "sinon": "^4.1.6",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "name": "first-server",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT"
}
